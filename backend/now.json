{
  "name": "onetap-backend",
  "version": 2,
  "builds": [{
    "src": "*.js",
    "use": "@now/node"
  }],
  "routes": [{ "src": "/.*", "dest": "/" }],
  "env": {
    "TWILIO_KEY": "@twilio-key",
    "AIRTABLE_KEY": "@airtable-key",
    "TWILIO_SID": "@twilio-sid",
    "TWILIO_NUMBER": "@twilio-number"
  }
}